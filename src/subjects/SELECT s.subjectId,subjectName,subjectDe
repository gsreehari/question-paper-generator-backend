SELECT s.subjectId,subjectName,subjectDegree,subjectScheme,subjectBranch,subjectYear,subjectSemister,questionCount,subjectStatus,subjectUnits,subjectCode, subjectType,subjectCreatedAt,u.userDisplayName,users.userDisplayName as addedBy FROM subject as s LEFT OUTER JOIN subjectFaculty as sf on s.subjectId = sf.subjectId LEFT OUTER JOIN users as u on u.userId = sf.userId LEFT OUTER JOIN users on users.userId = s.subjectAddedBy ORDER BY s.subjectCreatedAt DESC